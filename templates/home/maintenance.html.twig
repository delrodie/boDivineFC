                                        <div class="gdlr-core-pbf-column-content clearfix gdlr-core-js " style="max-width: 650px;">
                                            <div class="gdlr-core-pbf-element">
                                                <div class="gdlr-core-divider-item gdlr-core-item-pdlr gdlr-core-item-pdb gdlr-core-divider-item-normal" style="padding-bottom: 33px;">
                                                    <div class="gdlr-core-divider-line gdlr-core-skin-divider" style="border-bottom-width: 3px;"></div>
                                                </div>
                                            </div>
                                            <div class="gdlr-core-pbf-element">
                                                <div style="text-align:center;">
                                                    {% for label, messages in app.flashes %}
                                                        {% for message in messages %}
                                                            <div class="alert alert-{{ label }} alert-dismissible fade show d-flex text-center" role="alert" style="color: yellow; text-align:center; font-size: 2rem;">
                                                                {{ message }}
                                                            </div>
                                                        {% endfor %}
                                                    {% endfor %}
                                                </div>
                                                <div class="gdlr-core-text-box-item gdlr-core-item-pdlr gdlr-core-item-pdb gdlr-core-center-align">
