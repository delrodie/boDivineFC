                                    {% for equipe in equipes %}
                                        <div class="gdlr-core-personnel-list-column  gdlr-core-column-20 {% if loop.first %} gdlr-core-column-first {% endif%} gdlr-core-item-pdlr">
                                            <div class="gdlr-core-personnel-list clearfix">
                                                <div class="gdlr-core-personnel-list-image gdlr-core-media-image  gdlr-core-opacity-on-hover gdlr-core-zoom-on-hover">
                                                    <a href="#"><img src="{{ absolute_url(asset('uploads/equipe/'~ equipe.media)) }}" alt="{{ equipe.nom }}" width="600" height="465"></a>
                                                </div>
                                                <div class="gdlr-core-personnel-list-content-wrap">
                                                    <h3 class="gdlr-core-personnel-list-title" style="font-weight: 600;"><a href="#">{{ equipe.nom }}</a></h3>
                                                    <div class="gdlr-core-personnel-list-position gdlr-core-info-font gdlr-core-skin-caption" style="font-size: 15px;font-style: normal;">{{ equipe.fonction }}</div>
                                                    <div class="gdlr-core-personnel-list-divider gdlr-core-skin-divider"></div>
                                                    <div class="gdlr-core-personnel-list-content">
                                                        <p><a href="mailto:{{ equipe.email }}">{{ equipe.email }}</a></p>
                                                    </div>
                                                    <div class="gdlr-core-personnel-list-social">
                                                        <div class="gdlr-core-social-network-item gdlr-core-item-pdb  gdlr-core-none-align" style="padding-bottom: 0px;">
                                                        {% if equipe.linkedin %}
                                                            <a href="{{ equipe.linkedin }}" target="_blank" class="gdlr-core-social-network-icon" title="Linkedin"><i class="fa fa-linkedin"></i></a>
                                                        {% endif %}
                                                        {% if equipe.facebook %}
                                                            <a href="{{  equipe.facebook }}" target="_blank" class="gdlr-core-social-network-icon" title="facebook"><i class="fa fa-facebook"></i></a>
                                                        {% endif %}
                                                        {% if equipe.twitter %}
                                                            <a href="{{ equipe.twitter }}" target="_blank" class="gdlr-core-social-network-icon" title="Twitter"><i class="fa fa-twitter"></i></a>
                                                        {% endif %}
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {% endfor %}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
